        <script>
            function doalert(e){
               Swal.fire({
                        title: 'Are you sure?',
                        text: "Log out !",
                        icon: 'warning',
                        showCancelButton: true,
                        confirmButtonColor: '#A19391',
                        cancelButtonColor: '#A19391',
                        confirmButtonText: 'Yes, Log out!'
                        }).then((result) => {
                             if (result.isConfirmed) {
                                            $.ajax({
                                                url:'/logout',
                                                method:'get',
                                                success:(response)=>{
                                                    location.reload()
                                                }
                                            })
                                                    }
                        })
                     }
        </script>
